{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run mainwindow",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mainwindow",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "text": "handle SIG40 noprint nostop"
                }
            ],
            "logging": {
                "engineLogging": false,
                "trace": false,
                "traceResponse": false
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
